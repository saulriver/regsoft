<br/><br/><br/>
<h1>Managefiles</h1>

<table>
  <thead>
    <tr>
      <th>Incidentmanagement</th>
      <th>Filetype</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managefiles.each do |managefile| %>
      <tr>
        <td><%= managefile.incidentmanagement.id %></td>
        <td><%= managefile.filetype %></td>
        <td><%= managefile.state %></td>
        <td><%= link_to 'Show', managefile %></td>
        <td><%= link_to 'Edit', edit_managefile_path(managefile) %></td>
        <td><%= link_to 'Destroy', managefile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Managefile', new_managefile_path %>
